<% include ../partials/header %>
<div class="container">
    <h1>Team : <%=detail.team_name%></h1>
    <form action="/classsave" method="POST">

        <div class="form-group">
          <label for="">ID</label>
          <input type="text" class="form-control" value="<%=detail.team_id%>" id="" aria-describedby="helpId" readonly
            placeholder="">
          <small id="helpId" class="form-text text-muted">Help text</small>
        </div>
        <div class="form-group">
          <label for="">Team Name</label>
          <input type="text" class="form-control" value="<%=detail.team_name%>" id="" readonly
            aria-describedby="helpId" placeholder="">
          <small id="helpId" class="form-text text-muted">Help text</small>
        </div>
        <div class="form-group">
          <label for="">Team Players</label>
          <input type="text" class="form-control" value="<%=detail.team_players%>"  id="" aria-describedby="helpId" readonly
            placeholder="">
          <small id="helpId" class="form-text text-muted">Help text</small>
        </div>
        <div class="form-group">
            <label for="">Founded</label>
            <input type="text" class="form-control" value="<%=detail.Founded%>" id="" aria-describedby="helpId" readonly
              placeholder="">
            <small id="helpId" class="form-text text-muted">Help text</small>
          </div>
          <a name="" id="" class="btn btn-primary" href="/team" role="button">Back</a>
      </form>


   
</div>
<% include ../partials/footer %>
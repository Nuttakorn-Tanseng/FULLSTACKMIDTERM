<% include ../partials/header %>
<div class="container">
<h1>Add Team</h1>
  <form action="/addteamadd" method="POST">
    <div class="form-group">
      <label for="">ID</label>
      <input type="text" class="form-control" value="" name="id" id="" aria-describedby="helpId"
        placeholder="">
      <small id="helpId" class="form-text text-muted">Help text</small>
    </div>
    <div class="form-group">
      <label for="">Team Name</label>
      <input type="text" class="form-control" value="" name="name" id=""
        aria-describedby="helpId" placeholder="">
      <small id="helpId" class="form-text text-muted">Help text</small>
    </div>
    <div class="form-group">
      <label for="">Team Players</label>
      <input type="text" class="form-control" value="" name="players" id="" aria-describedby="helpId"
        placeholder="">
      <small id="helpId" class="form-text text-muted">Help text</small>
    </div>
    <div class="form-group">
      <label for="">Founded</label>
      <input type="text" class="form-control" value="" name="founded" id="" aria-describedby="helpId"
        placeholder="">
      <small id="helpId" class="form-text text-muted">Help text</small>
    </div>
    <button type="submit" class="btn btn-primary">Add</button>
  </form>
</div>
<% include ../partials/footer %>